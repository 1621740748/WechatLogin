<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>OpenIDAction</servlet-name>
  	<servlet-class>com.tnz.action.OpenIDAction</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>OpenIDAction</servlet-name>
  	<url-pattern>/wechat/acquireOpenID.do</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>CallBackAction</servlet-name>
  	<servlet-class>com.tnz.action.CallBackAction</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CallBackAction</servlet-name>
  	<url-pattern>/wechat/callBack.do</url-pattern>
  </servlet-mapping>
</web-app>
